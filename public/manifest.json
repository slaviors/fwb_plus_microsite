    },
    {
      "src": "/images/assets/logo/Logo FWB PNG.png",
      "sizes": "512x512", 
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/images/assets/logo/Logo FWB logomark PNG Trnsparan.png",
      "sizes": "96x96",
      "type": "image/png", 
      "purpose": "any"
    },
    {
      "src": "/images/assets/logo/Brand Identity FWB.png",
      "sizes": "180x180",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "shortcuts": [
    {
      "name": "Konsultasi Event",
      "short_name": "Konsultasi",
      "description": "Konsultasi gratis untuk kebutuhan event Anda",
      "url": "/konsultasi",
      "icons": [
        {
          "src": "/images/services/corporate-event.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Portfolio Event", 
      "short_name": "Portfolio",
      "description": "Lihat galeri event yang telah kami kerjakan",
      "url": "/portfolio",
      "icons": [
        {
          "src": "/images/gallery/event1.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Layanan Event",
      "short_name": "Layanan", 
      "description": "Jelajahi berbagai layanan event organizer kami",
      "url": "/layanan",
      "icons": [
        {
          "src": "/images/services/wedding-event.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Hubungi Kami",
      "short_name": "Kontak",
      "description": "Hubungi tim FWB+ untuk informasi lebih lanjut", 
      "url": "/kontak",
      "icons": [
        {
          "src": "/images/assets/logo/Logo FWB logomark PNG.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "web",
      "url": "https://links.fwbplus.id"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/kontak?email=%s"
    },
    {
      "protocol": "tel", 
      "url": "/kontak?phone=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/upload-proposal",
      "accept": {
        "application/pdf": [".pdf"],
        "application/msword": [".doc"],
        "application/vnd.openxmlformats-officedocument.wordprocessingml.document": [".docx"],
        "image/jpeg": [".jpg", ".jpeg"],
        "image/png": [".png"]
      }
    }
  ],
  "share_target": {
    "action": "/share-event",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "event-files",
          "accept": [
            "image/jpeg",
            "image/png",
            "image/webp",
            "application/pdf"
          ]
        }
      ]
    }
  },
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "minimal-ui",
    "browser"
  ],
  "id": "fwb-plus-event-organizer"
}